<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
	<servlet>
		<description>
		</description>
		<display-name>WriteReviewServlet</display-name>
		<servlet-name>WriteReviewServlet</servlet-name>
		<servlet-class>com.bookstore.controller.frontend.review.WriteReviewServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>SubmitReviewServlet</display-name>
		<servlet-name>SubmitReviewServlet</servlet-name>
		<servlet-class>com.bookstore.controller.frontend.review.SubmitReviewServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WriteReviewServlet</servlet-name>
		<url-pattern>/WriteReviewServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SubmitReviewServlet</servlet-name>
		<url-pattern>/SubmitReviewServlet</url-pattern>
	</servlet-mapping>


  <error-page>
    <error-code>404</error-code>
    <location>/error/404.jsp</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/error/500.jsp</location>
  </error-page>

</web-app>